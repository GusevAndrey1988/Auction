{
    "name": "gusevandrey/demo-auction",
    "type": "project",
    "authors": [
        {
            "name": "Gusev Andrey",
            "email": "gusevandrey1988@gmail.com"
        }
    ],
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "overtrue/phplint": "^9.0",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "app": "php bin/app.php --ansi"
    }
}
